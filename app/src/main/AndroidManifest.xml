<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.composeworkshop">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeWorkShop">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ComposeWorkShop.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Scaffold" />
        <activity android:name=".CrossFadeAnimation" />
        <activity android:name=".ConstraintLayout" />
        <activity android:name=".Canvas" />
        <activity android:name=".DropDown"/>
        <activity android:name=".DropDownList"/>
        <activity android:name=".AnimationCard"/>
    </application>
</manifest>